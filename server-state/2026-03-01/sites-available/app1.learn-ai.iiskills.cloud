server {
    listen 80;
    listen [::]:80;
    server_name app1.learn-ai.iiskills.cloud;

    return 301 https://learn-ai.iiskills.cloud$request_uri;
}

server {
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    server_name app1.learn-ai.iiskills.cloud;

    ssl_certificate     /etc/ssl/iiskills/fullchain.pem;
    ssl_certificate_key /etc/ssl/iiskills/privkey.pem;

    return 301 https://learn-ai.iiskills.cloud$request_uri;
}
