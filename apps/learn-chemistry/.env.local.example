# Environment Variables Example for Learn AI
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# App Configuration
NEXT_PUBLIC_SUPABASE_SUSPENDED=false
NEXT_PUBLIC_SITE_URL=http://localhost:3024
NEXT_PUBLIC_MAIN_DOMAIN=iiskills.cloud
NEXT_PUBLIC_APP_ID=learn-chemistry
NEXT_PUBLIC_DISABLE_AUTH=true

# External API Keys
NEWS_API_KEY=your-newsapi-key-here
OPENAI_API_KEY=your-openai-key-here
SENDGRID_API_KEY=your-resend-key-here

# External Services
JOBS_FEED_URL=https://example.com/api/jobs
PAYMENT_RETURN_URL=https://aienter.in/payments
