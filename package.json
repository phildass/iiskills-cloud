{
  "name": "iiskills-cloud",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "yarn workspace @iiskills/web dev",
    "dev:admin": "yarn workspace @iiskills/admin dev",
    "dev:learn-physics": "yarn workspace @iiskills/learn-physics dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "turbo": "^1.13.4"
  }
}
